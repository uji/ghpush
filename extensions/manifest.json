{
  "name": "ghpush",
  "description": "notify github`s notifications",
  "version": "1.0.0",
  "browser_action": {
    "default_icon": "icon128.png",
    "default_popup": "./dist/index.html"
  },
  "permissions": [
    "notifications",
    "alarms"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "manifest_version": 2
}
